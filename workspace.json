{
  "version": 2,
  "cli": { "defaultCollection": "@nrwl/node" },
  "defaultProject": "node",
  "projects": {
    "lib1": {
      "root": "libs/lib1",
      "sourceRoot": "libs/lib1/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["libs/lib1/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/lib1"],
          "options": {
            "jestConfig": "libs/lib1/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "node": {
      "root": "apps/node",
      "sourceRoot": "apps/node/src",
      "projectType": "application",
      "prefix": "node",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/node",
            "main": "apps/node/src/main.ts",
            "tsConfig": "apps/node/tsconfig.app.json",
            "assets": ["apps/node/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/node/src/environments/environment.ts",
                  "with": "apps/node/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "x": {
          "executor": "nx-plugin-esbuild:build",
          "options": {
            "outputPath": "dist-test/apps/node",
            "main": "apps/node/src/main.ts",
            "tsConfig": "apps/node/tsconfig.app.json",
            "watch": true
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": { "buildTarget": "node:build" }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/node/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/node"],
          "options": {
            "jestConfig": "apps/node/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "other-aaa": {
      "root": "apps/other/aaa",
      "sourceRoot": "apps/other/aaa/src",
      "projectType": "application",
      "prefix": "other-aaa",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/other/aaa",
            "main": "apps/other/aaa/src/main.ts",
            "tsConfig": "apps/other/aaa/tsconfig.app.json",
            "assets": ["apps/other/aaa/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/other/aaa/src/environments/environment.ts",
                  "with": "apps/other/aaa/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": { "buildTarget": "other-aaa:build" }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/other/aaa/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/other/aaa"],
          "options": {
            "jestConfig": "apps/other/aaa/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "app1": {
      "root": "apps/app1",
      "sourceRoot": "apps\\app1\\src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/app1",
            "main": "apps/app1/src/main.ts",
            "tsConfig": "apps/app1/tsconfig.app.json",
